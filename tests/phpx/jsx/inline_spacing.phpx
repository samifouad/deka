/*
TEST: JSX inline spacing between text and component
Covers: preserving inline space in "Hello <Comp />" output while retaining class/style attrs.
*/

import { renderToString } from 'component/dom'

function Hello($props) {
  return <span class="inner" style="color:red">{$props.name}</span>
}

$node = <div class="outer">Hello <Hello name="hi" /></div>
echo renderToString($node) . "\n"
