/*
TEST: encoding/binary supports endian encode/decode and append helpers
*/
import { u16_be, read_u16_be, read_u32_le, append_u32_le } from 'encoding/binary'

$bytes = u16_be(513)
$bytes = append_u32_le($bytes, 16909060)

$r1 = read_u16_be($bytes, 0)
$r2 = read_u32_le($bytes, 2)

echo ($r1.ok ? $r1.value : -1) . ':' . ($r2.ok ? $r2.value : -1) . "\n"
