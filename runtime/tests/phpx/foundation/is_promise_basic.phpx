/*
TEST: is_promise() detects runtime Promise values
Covers: promise-vs-plain-value detection at runtime.
*/

async function makeValue(): Promise<int> {
  return 1
}

$pending = makeValue()
echo (is_promise($pending) ? 'yes' : 'no') . "\n"

$resolved = await $pending
echo $resolved . "\n"
echo (is_promise($resolved) ? 'yes' : 'no') . "\n"
