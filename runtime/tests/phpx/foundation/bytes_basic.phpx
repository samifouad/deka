/*
TEST: bytes module exposes core byte helpers through stable API
*/
import { from_string, to_string, len, get, set, slice, concat } from 'bytes'

$b = from_string('abc')
$b = set($b, 1, ord('Z'))

$out = concat(slice($b, 0, 2), from_string('!'))

echo to_string($out) . ':' . len($out) . ':' . get($b, 1) . "\n"
