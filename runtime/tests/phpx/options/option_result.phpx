/*
TEST: Option/Result core behavior
Covers: Some/None, Ok/Err, unwrap, unwrap_or.
*/

import { option_some, option_none } from 'core/option';
import { result_ok, result_err } from 'core/result';

$opt = option_some(5);
$none = option_none();

$result = result_ok('ok');
$error = result_err('fail');

echo ($opt->is_some() ? 'some' : 'none') . "\n";
echo $opt->unwrap() . "\n";
echo $none->unwrap_or(9) . "\n";

echo $result->unwrap() . "\n";
echo $error->unwrap_or('fallback') . "\n";
