import { get_mode, toggle_mode, get_perms, echo_perms } from '@user/enum_flags' as wasm;

$mode = get_mode();
$mode2 = toggle_mode($mode);

$perms = get_perms();
$perms2 = echo_perms(['read', 'exec']);
$perms3 = echo_perms(5); // read + exec

echo "mode={$mode}\n";
echo "mode2={$mode2}\n";
echo "perms=" . implode(',', $perms) . "\n";
echo "perms2=" . implode(',', $perms2) . "\n";
echo "perms3=" . implode(',', $perms3) . "\n";
