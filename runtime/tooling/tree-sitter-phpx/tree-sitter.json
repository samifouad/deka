{
  "grammars": [
    {
      "name": "phpx",
      "camelcase": "PHP",
      "scope": "source.phpx",
      "path": "php",
      "external-files": "common/scanner.h",
      "file-types": [
        "php",
        "php3",
        "php4",
        "php5",
        "php7",
        "phps",
        "phpt",
        "phtml"
      ],
      "highlights": "queries/highlights.scm",
      "injections": [
        "queries/injections.scm",
        "queries/injections-text.scm"
      ],
      "tags": "queries/tags.scm"
    },
    {
      "name": "phpx_only",
      "camelcase": "PHPX_Only",
      "scope": "source.phpx",
      "path": "php_only",
      "external-files": "common/scanner.h",
      "file-types": [
        "phpx"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "tags": "queries/tags.scm"
    }
  ],
