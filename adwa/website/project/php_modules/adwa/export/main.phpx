import { cmd_args, env_set } from '../lib.phpx'
import { stdout } from 'io'

$args = cmd_args()
if (count($args) < 1) {
    stdout("export: KEY=VALUE required\n")
    return
}
foreach ($args as $arg) {
    $pair = explode('=', '' . $arg, 2)
    if (count($pair) !== 2) {
        stdout('export: invalid pair ' . $arg . "\n")
        continue
    }
    env_set($pair[0], $pair[1])
}
