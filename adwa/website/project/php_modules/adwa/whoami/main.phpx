import { env_all } from '../lib.phpx'
import { stdout } from 'io'

$env = env_all()
$name = 'adwa'
if (is_array($env) && array_key_exists('USER', $env) && $env['USER'] !== '') {
    $name = '' . $env['USER']
} else if (is_array($env) && array_key_exists('USERNAME', $env) && $env['USERNAME'] !== '') {
    $name = '' . $env['USERNAME']
}
stdout($name . "\n")
